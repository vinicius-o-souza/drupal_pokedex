{
  "name": "pokemon",
  "private": true,
  "scripts": {
    "livereload": "livereload --exclusions node_modules/ --exts 'css,js,twig,theme,apng,avif,gif,jpg,jpeg,jfif,pjpeg,pjp,png,svg,webp'",
    "sass-dev": "sass --no-source-map --style=expanded scss:css",
    "sass-prod": "sass --no-source-map --style=compressed scss:css"
  },
  "devDependencies": {
    "livereload": "^0.9.3"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "gulp-cli": "^2.3.0",
    "sass": "^1.69.5"
  }
}
